<jboss xmlns:wl="http://www.bea.com/ns/weblogic/90">
<enterprise-beans>
<session>
<ejb-name>ItemLookupBean</ejb-name>
<jndi-name>ejb/ItemLookupBean</jndi-name>
<local-jndi-name>java:ejb/ItemLookupBean</local-jndi-name>
</session>
<session>
<ejb-name>ProductCatalogBean</ejb-name>
<jndi-name>ejb/ProductCatalogBean</jndi-name>
<local-jndi-name>java:ejb/ProductCatalogBean</local-jndi-name>
</session>
<message-driven>
